{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "outDir": "./dist",
    "rootDir": "src",
    // "allowImportingTsExtensions": true,
    "declaration": false,
    "emitDeclarationOnly": false,
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "src/**/*",
    "**/*.ts",
  ],
  "exclude": ["node_modules", "dist"]
}
